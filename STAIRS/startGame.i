void movingScreen() {

    if (isOnMenu) {
        goingOnMenu = 1;

        if (SG_hOff > 0) {


            SG_hOff -= 5;


            character_S1.worldCol += 5;
            character_T.worldCol += 5;
            character_A.worldCol += 5;
            character_I.worldCol += 5;
            character_R.worldCol += 5;
            character_S2.worldCol += 5;

        } else {

            isOnMenu = 0;
            goingOnMenu = 0;
        }
    } else {

        goingOnMenu = 1;

        if (SG_hOff < 480 / 2) {


            SG_hOff += 5;


            character_S1.worldCol -= 5;
            character_T.worldCol -= 5;
            character_A.worldCol -= 5;
            character_I.worldCol -= 5;
            character_R.worldCol -= 5;
            character_S2.worldCol -= 5;

        } else {

            isOnMenu = 1;
            goingOnMenu = 0;
        }
    }

}
