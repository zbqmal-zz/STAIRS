    if (player.screenCol > 0) {
        shadowOAM[6].attr0 = player.screenRow | (2<<14);
        shadowOAM[6].attr1 = player.screenCol | (2<<14);
        shadowOAM[6].attr2 = ((4 * player.curFrame)*32+(4 + 2 * player.aniState));
    }
}


void initHighlighter() {


    highlighter.screenRow = 0;
    highlighter.screenCol = 0;


    highlighter.width = 16;
    highlighter.height = 16;




    highlighter.active = 0;
    menuOption = NONE;
}

void updateHighlighter() {


    highlighter.active = 0;
    buttonB.active = 0;


    if (player.screenRow > 80 && player.screenRow < 110 && player.screenCol > 125 && player.screenCol < 155) {


        highlighter.screenRow = 55;
        highlighter.screenCol = 105;


        highlighter.active = 1;
        buttonB.active = 1;


        menuOption = MENU_INSTRUCTION;
    }


    if (player.screenRow > 70 && player.screenRow < 110 && player.screenCol > 195 && player.screenCol < 210) {


        highlighter.screenRow = 10;
        highlighter.screenCol = 175;


        highlighter.active = 1;
        buttonB.active = 1;


        menuOption = MENU_GAMESTART;
    }
}

void drawHighlighter() {

    if (highlighter.active && isOnMenu) {
        shadowOAM[7].attr0 = highlighter.screenRow | (0<<14);
        shadowOAM[7].attr1 = highlighter.screenCol | (1<<14);
        shadowOAM[7].attr2 = ((24)*32+(0));
    } else {
        shadowOAM[7].attr0 = (2<<8);
    }
}


void initButtonB() {


    buttonB.screenRow = 0;
    buttonB.screenCol = 0;


    buttonB.width = 8;
    buttonB.height = 8;


    buttonB.active = 0;
}

void updateButtonB() {


    buttonB.screenRow = player.screenRow + player.height + 3;
    buttonB.screenCol = player.screenCol;
}

void drawButtonB() {

    if (buttonB.active) {

        shadowOAM[35].attr0 = buttonB.screenRow | (0<<14);
        shadowOAM[35].attr1 = buttonB.screenCol | (1<<14);
        shadowOAM[35].attr2 = ((28)*32+(30));

    } else {

        shadowOAM[35].attr0 = (2<<8);
    }
}


